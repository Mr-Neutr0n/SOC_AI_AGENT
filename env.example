# SOC AI Agent Environment Configuration
# Copy this file to .env and fill in your actual values

# Google Cloud Platform Configuration
GOOGLE_CLOUD_PROJECT=your-gcp-project-id
GOOGLE_APPLICATION_CREDENTIALS=/path/to/service-account-key.json

# Threat Intelligence API Keys
VIRUSTOTAL_API_KEY=your-virustotal-api-key
ABUSEIPDB_API_KEY=your-abuseipdb-api-key

# Optional: Additional Threat Intelligence Sources
# SHODAN_API_KEY=your-shodan-api-key
# HYBRID_ANALYSIS_API_KEY=your-hybrid-analysis-api-key

# Notification Webhooks
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/SLACK/WEBHOOK
TEAMS_WEBHOOK_URL=https://your-teams-webhook-url
EMAIL_WEBHOOK_URL=https://your-email-webhook-url

# Optional: Chronicle Security Operations
# CHRONICLE_SERVICE_ACCOUNT_INFO=your-chronicle-service-account-json

# Development/Testing Settings
# Set to 'true' for development mode
DEBUG_MODE=false
LOCAL_TESTING=false
MOCK_THREAT_INTEL=false

# Logging Configuration
LOG_LEVEL=INFO

# Performance Settings
MAX_CONCURRENT_ALERTS=10
BATCH_SIZE=50
CACHE_TTL=3600

# Security Settings
ENABLE_ENCRYPTION=true
MASK_SENSITIVE_DATA=true
AUDIT_LOGS=true 